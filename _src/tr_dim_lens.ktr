    <connection>sls_raw</connection>
    <sql>SELECT
l.lens_id
, l.name AS lens_name
, l.year AS lens_year
, l.lens_category_id
, lc.lens_category
, l.focal_length_min
, l.focal_length_max
, l.aperture_max
, l.aperture_min
, l.focusing_distance_min
, l.filter_size
FROM
sls_raw.lens l
LEFT JOIN
sls_raw.lens_category lc
ON
l.lens_category_id = lc.lens_category_id
</sql>
